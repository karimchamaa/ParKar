
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:map="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/back">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:text="343 Gold St, Brooklyn, NY 11201 "
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:background="#9b0000"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="100dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:text="delivery time: 12:01pm- 12:30pm   "
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:background="#9b0000"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="51sp"
        android:src="@drawable/loadingdock"
        android:visibility="visible"
        android:layout_height="51sp"
        android:layout_marginTop="100dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/textView3"
        android:layout_toEndOf="@+id/textView3"
        android:id="@+id/LoadingDock1"
        android:onClick="onReserve"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:text="55 Clark St Brooklyn, NY 11201"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:background="#009b00"
        android:layout_marginTop="37dp"
        android:layout_below="@+id/LoadingDock1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:text="delivery time: 13:01pm-13:30pm   "
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:background="#009b00"
        android:layout_below="@+id/textView4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:src="@drawable/garage"
        android:visibility="visible"
        android:id="@+id/LoadingDock2"
        android:onClick="onReserve2"
        android:layout_height="100dp"
        android:layout_width="600dp"
        android:layout_marginTop="15dp"
        android:layout_below="@+id/LoadingDock1"
        android:layout_toRightOf="@+id/textView4"
        android:layout_toEndOf="@+id/textView4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView7"
        android:text="4 WSVillage, New York, NY 10012"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:background="#9b0000"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/LoadingDock2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:text="delivery time: 14:01pm- 14:30pm   "
        android:textSize="18sp"
        android:textStyle="normal|bold"
        android:textAllCaps="true"
        android:textColor="#000000"
        android:background="#9b0000"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="51sp"
        android:src="@drawable/loadingdock"
        android:visibility="visible"
        android:layout_height="51sp"
        android:id="@+id/LoadingDock3"
        android:onClick="onReserve3"
        android:layout_alignTop="@+id/textView7"
        android:layout_toRightOf="@+id/textView7"
        android:layout_toEndOf="@+id/textView7"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-1.5dp" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/reservation"
        android:visibility="visible"
        android:layout_marginTop="127dp"
        android:layout_below="@+id/LoadingDock3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <ImageView
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:src="@drawable/schedule"
        android:visibility="visible"
        android:layout_above="@+id/textView2"
        android:layout_toStartOf="@+id/LoadingDock2"
        android:layout_marginBottom="37dp" />


</RelativeLayout>